{
  "name": "do-not-track",
  "version": "0.2.0",
  "description": "A package to block web-requests to tracking-servers",
  "private": false,
  "main": "./lib/do-not-track",
  "typings": "./types",
  "scripts": {
    "analyze": "source-map-explorer lib/**/*.js",
    "build": "rollup --config",
    "dev": "npm run start",
    "lint:code": "eslint --no-error-on-unmatched-pattern ./src/**/*.ts",
    "lint:styles": "stylelint --allow-empty-input styles/*.{css,less}",
    "lint": "npm-run-all --parallel lint:*",
    "postinstall": "npm run build",
    "prepublishOnly": "npm run build",
    "start": "rollup --watch --config"
  },
  "keywords": [
    "do not track",
    "dnt",
    "no tracking",
    "tracking blocker"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-do-not-track"
  },
  "homepage": "https://atom.io/packages/do-not-track",
  "bugs": {
    "url": "https://github.com/idleberg/atom-do-not-track/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationHooks": [],
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "dependencies": {
    "browser-extension-url-match": "^0.3.3",
    "disposify": "^1.0.0",
    "multimatch": "^5.0.0",
    "svelte": "^3.32.1"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/atom": "^1.40.5",
    "@types/electron": "^1.6.10",
    "@types/node": "^14.14.17",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-plugin-json": "^2.1.2",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rollup": "^2.35.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "source-map-explorer": "^2.5.1",
    "stylelint": "^13.8.0",
    "svelte-preprocess": "^4.6.5",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.scss": [
      "prettier --write",
      "stylelint --fix"
    ]
  }
}
