{
  "name": "do-not-track",
  "version": "0.4.1",
  "description": "A package to block web-requests to tracking-servers",
  "private": false,
  "main": "./lib/index",
  "typings": "./types",
  "scripts": {
    "analyze": "source-map-explorer lib/**/*.js",
    "build": "npm run clean && rollup --config",
    "clean": "rimraf ./lib",
    "dev": "npm run start",
    "lint:code": "eslint --no-error-on-unmatched-pattern ./src/**/*.ts",
    "lint:styles": "stylelint --allow-empty-input styles/*.{css,less}",
    "lint": "npm-run-all --parallel lint:*",
    "prepublishOnly": "npm run build",
    "start": "rollup --watch --config"
  },
  "keywords": [
    "do not track",
    "dnt",
    "no tracking",
    "tracking blocker",
    "analytics",
    "google analytics",
    "matomo",
    "piwik",
    "telemetry",
    "metrica",
    "yandex metrica"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-do-not-track"
  },
  "homepage": "https://atom.io/packages/do-not-track",
  "bugs": {
    "url": "https://github.com/idleberg/atom-do-not-track/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationHooks": [],
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "dependencies": {
    "browser-extension-url-match": "^0.3.3",
    "disposify": "^1.0.0",
    "multimatch": "^5.0.0",
    "svelte": "^3.32.1"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/atom": "^1.40.5",
    "@types/electron": "^1.6.10",
    "@types/node": "^14.14.43",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.25.0",
    "eslint-plugin-json": "^2.1.2",
    "husky": ">=4 <5",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.12",
    "source-map-explorer": "^2.5.1",
    "stylelint": "^13.13.0",
    "svelte-preprocess": "^4.6.5",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.scss": [
      "stylelint --cache --fix",
      "prettier --write"
    ]
  }
}
